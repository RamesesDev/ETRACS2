<schema>
	<element name="classification">
		<field name="classId" required="true"  />
		<field name="classCode" required="true"  />
		<field name="classTitle" required="true"  />
	</element>
	
	<element name="actualUse">
		<field name="actualUseId" required="true"  />
		<field name="actualUseCode" required="true"  />
		<field name="actualUseTitle" required="true"  />
	</element>
	
	<element name="specificClass">
		<field name="specificClassId" required="true"  />
		<field name="specificClassCode" required="true"  />
		<field name="specificClassTitle" required="true"  />
	</element>
	
	<element name="subclass">
		<field name="subclassId" required="true"  />
		<field name="subclassCode" required="true"  />
		<field name="subclassTitle" required="true"  />
	</element>
	
	<element name="assessment">
		<field name="marketValue" required="true" type="decimal" />
		<field name="adjMarketValue" required="true" type="decimal" />
		<field name="assessLevel" required="true" type="decimal" />
		<field name="assessedValue" required="true" type="decimal" />
	</element>
	
	<element name="exemption">
		<field name="exemptId"  />
		<field name="exemptCode"  />
		<field name="exemptTitle"  />
	</element>
	
	<element name="cancelInfo">
		<field name="cancelReason"  />
		<field name="cancelDate" />
		<field name="cancelledByTdNo"  />
	</element>

	<!-- Title -->
	<element name="title">
		<field name="titleNo"  />
		<field name="titleDate" />
		<field name="titleType"  />
	</element>
	
	<!-- Name Info -->
	<element name="taxpayer">
		<field name="taxpayerId"  />
		<field name="taxpayerNo" />
		<field name="taxpayerName" required="true"  />
		<field name="taxpayerAddress"  />
	</element>
	
	<!-- Signatory -->
	<element name="signatory">
		<field name="name" required="true" />
		<field name="title" required="true" />
		<field name="dtSigned" required="true"/>
	</element>
	
	<!-- PreviousInfo -->
	<element name="previousInfo">
		<field name="prevTdNo"  />
		<field name="prevPin"  />
		<field name="prevOwner" />
		<field name="prevAssessedValue" required="true" type="decimal" />
		<field name="prevMarketValue" required="true" type="decimal" />
	</element>

	<element name="param">
		<field name="paramId" required="true" />
		<field name="paramCode" required="true" />
		<field name="paramName" required="true" />
		<field name="paramValue" required="true" />
	</element>

</schema>	
