<schema>
	<element name="rp" >
		<field name="objid" primary="true" />
		<field name="schemaName" required="true"/>
		<field name="schemaVersion" required="true"/>
		<field name="state"  required="true" />
		<complex name="info" ref="info" serializer="default" />
	</element>
	
	<element name="info">
		<field name="txnType" required="true" />
		<field name="dtCreated" />
		<field name="dtApproved" />
		<field name="provCity" required="true" />
		<field name="muniDistrict" required="true" />
		<field name="barangay" required="true" />
		<field name="pin" required="true" />
		<field name="provCityIndex" required="true"  />
		<field name="muniDistrictIndex" required="true"  />
		<field name="brgyIndex" required="true"  />
		<field name="sectionNo" required="true"  />
		<field name="parcelNo" required="true"  />
		<field name="claimNo" required="true" type="integer" />
		<field name="cadastralLotNo" required="true"  />
		<field name="blockNo"  />
		<field name="surveyNo"  />
		<field name="titleNo"  />
		<field name="houseNo"  />
		<field name="street"  />
		<field name="purok"  />
		<field name="barangayId" required="true"  />
		<field name="prevRealPropertyId"  />
		<field name="mainPropertyId"  />
		<field name="attribute"  />
		<complex name="boundary" ref="boundary" serializer="default" required="true" />
		<complex name="cancelInfo" ref="common:cancelInfo" serializer="default" required="false" />
	</element>
	
	<element name="boundary">
		<field name="north" required="true"  />
		<field name="south" required="true"  />
		<field name="east" required="true"  />
		<field name="west" required="true"  />
	</element>
	
</schema>	
