<schema>
	<element name="tmcr" tablename="tmcr">
		<field name="objid" primary="true"/>
		<field name="schemaName" required="true"/>
		<field name="schemaVersion" required="true"/>
		<field name="state" required="true"/>
		<field name="provCity" required="true"/>
		<field name="muniDistrict" required="true"/>
		<field name="barangay" required="true"/>
		<field name="section" required="true"/>
		<complex name="info" ref="info" required="true" serializer="default" />
	</element>
	
	<element name="info">
		<field name="tdNo" required="true"/>
		<field name="declarant" required="true"/>
		<field name="address" required="true"/>
		<field name="cadastralLotNo" required="true" />
		<field name="surveyNo" />
		<field name="titleNo" />
		<field name="areaSqm" required="true" type="decimal" />
		<field name="areaHec" required="true" type="decimal" />
		<field name="kind" required="true" />
		<field name="class" required="true" />
		<!-- must be the value of amv -->
		<field name="mv" required="true" type="decimal" />  
		<field name="av" required="true" type="decimal" />
		<field name="remarks" />
		<field name="status" />
	</element>
</schema>
