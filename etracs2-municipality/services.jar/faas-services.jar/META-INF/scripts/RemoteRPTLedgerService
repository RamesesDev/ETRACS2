import com.rameses.annotations.*;

public class RemoteRPTLedgerService {

	@Service("OldETRACSService")
	def oldService;
	
	@ProxyMethod
	@Async(type="topic")
	public def findLedger( params ) {
		println "EXECUTING REMOTE...$params";
		oldService.findLedgerByTdId2( params );
	}
	
	@ProxyMethod
	@Async(type="topic")
	public def executeTest( params ) {
		return oldService.test( params );
	}

}
