<schema>

    <element name="terminal" tablename="terminal" >
		<field name="terminalid" primary="true"/>
		<field name="parentid" required="true" />
		<field name="parentcode" required="true" />
		<field name="parenttype" />
		<field name="macaddress"/>
		<field name="dtregistered"  />
		<field name="registeredby" />
		<field name="info"/>
		<field name="state" default="0"/>
    </element>

    <element name="blocked" tablename="blocked_terminal" >
		<field name="terminalid" primary="true"/>
		<field name="userid" primary="true" />
	</element>	
</schema>