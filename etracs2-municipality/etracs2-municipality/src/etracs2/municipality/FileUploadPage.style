"doUpload" [uploading == true ] {
    enabled:false;
}

"doUpload" [uploading == false] {
    enabled:true;
}

"doClose" [status != 'done' ] {
    enabled:false;
}

"doClose" [status == 'done'] {
    enabled:true;
}

"info.*" [uploading == true ] {
    enabled:false;
}

"info.*" [uploading == false] {
    enabled:true;
}

"file" [uploading == true ] {
    enabled:false;
}

"file" [uploading == false] {
    enabled:true;
}