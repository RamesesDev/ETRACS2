"entity.tax.*" [entity.info.mode=="ONLINE"] {
    enabled: false;
}

"entity.tax.*" [entity.info.mode!="ONLINE"] {
    enabled: true;
}

"entity.info.payer.(lastname|firstname|middlename)" [taxpayer!=null] {
    enabled: false;
}

"entity.info.payer.(lastname|firstname|middlename)" [taxpayer==null] {
    enabled: true;
}

