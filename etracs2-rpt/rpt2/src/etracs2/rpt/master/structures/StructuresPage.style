"entity.*" [mode == 'view'] {
    readonly:true;
}

"entity.*" [mode != 'view'] {
    readonly:false;
}

"entity.objid" [mode == 'create'] {
    readonly:false;
}

"entity.objid" [mode != 'create'] {
    readonly:true;
}

"materials" [mode != 'view' ]{
    enabled:true;
}

"materials" [mode == 'view']{
    enabled:false;
}