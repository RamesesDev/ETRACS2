"entity.*" [mode == 'view'] {
    readonly:true;
}

"entity.*" [mode != 'view'] {
    readonly:false;
}

"selectedItem|searchText" [mode == 'view'] {
    enabled:true;
}

"selectedItem|searchText" [mode != 'view'] {
    enabled:false;
}

"entity.objid" [mode == 'create'] {
    readonly:false;
}

"entity.objid" [mode != 'create'] {
    readonly:true;
}

"structures" [mode != 'view' ]{
    enabled:true;
}

"structures" [mode == 'view']{
    enabled:false;
}