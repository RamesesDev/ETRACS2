"entity.*|taxpayer|selectedProperty" [mode == 'view'] {
    readonly:true;
}

"entity.*|taxpayer|selectedProperty" [mode != 'view'] {
    readonly:false;
}

"selectedItem|searchText" [mode == 'view'] {
    enabled:true;
}

"selectedItem|searchText" [mode != 'view'] {
    enabled:false;
}

"entity.objid" [mode == 'create'] {
    readonly:false;
}

"entity.objid" [mode != 'create'] {
    readonly:true;
}
