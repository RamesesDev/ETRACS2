"entity.*" [mode == 'view'] {
    readonly:true;
}

"entity.*" [mode != 'view'] {
    readonly:false;
}

"selectedItem|searchText|selectedOwner" [mode == 'view'] {
    enabled:true;
}

"selectedItem|searchText|selectedOwner" [mode != 'view'] {
    enabled:false;
}

"entity.objid" [mode == 'create'] {
    readonly:false;
}

"entity.objid" [mode != 'create'] {
    readonly:true;
}
