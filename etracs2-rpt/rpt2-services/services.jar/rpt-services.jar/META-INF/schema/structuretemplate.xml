<schema>
	<element name="structuretemplate" tablename="structuretemplate">
		<field name="objid" primary="true"/>
		<field name="schemaname" required="true"/>
		<field name="schemaversion" required="true"/>
		<field name="templatecode" required="true"/>
		<complex name="structures" ref="structure" type="list" serializer="default" />
	</element>
	
	<element name="structure">
		<field name="objid" />
		<field name="structurecode" />
		<field name="structuredesc" />
	</element>
</schema>
