<schema>
	<element name="structures" tablename="structures">
		<field name="objid" primary="true"/>
		<field name="schemaname" required="true"/>
		<field name="schemaversion" required="true"/>
		<field name="structurecode" required="true"/>
		<field name="structuredesc" required="true"/>
		<field name="indexno" type="integer" />
		<complex name="materials" ref="material" type="list" serializer="default" />
	</element>
	
	<element name="material">
		<field name="objid" />
		<field name="materialcode" />
		<field name="materialdesc" />
	</element>
</schema>
