<schema>
	<element name="account" tablename="account" >
		<field name="objid" primary="true"/>
		<field name="schemaname" required="true"/>
		<field name="schemaversion" required="true"/>
		<field name="fromyear" type="integer" required="true" />
		<field name="toyear" type="integer" />
		<field name="code" required="true"/>
		<field name="title" required="true"/>
		<!-- VALID VALUES : NGAS, SRE -->
		<field name="charttype" required="true"/> 
		<!-- VALID VALUES: CHART, CATEGORY, GLACCOUNT, SUBACCOUNT -->
		<field name="type" required="true"/>   
		<field name="level" type="integer" required="true"/>
		<field name="parentid" />
		<!-- reference to chart record -->		
		<field name="rootid"/>
		<link ref="sysinfo:sysinfo"/>
	</element>
</schema>