"entity.*|bank" [mode == 'view'] {
    readonly:true;
}

"entity.*|bank" [mode != 'view'] {
    readonly:false;
}

"entity.objid" [mode == 'create'] {
    readonly:false;
}

"entity.objid" [mode != 'create'] {
    readonly:true;
}

"entity.bankname" [mode != 'view'] {
    readonly:true;
}

"entity.branchname" [mode != 'view'] {
    readonly:true;
}
