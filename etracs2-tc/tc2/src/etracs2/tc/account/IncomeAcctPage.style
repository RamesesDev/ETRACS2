"entity.*" [mode == 'view'] {
    readonly:true;
}

"entity.*" [mode != 'view'] {
    readonly:false;
}

"selectedItem|searchText" [mode == 'view'] {
    enabled:true;
}

"selectedItem|searchText" [mode != 'view'] {
    enabled:false;
}


"entity.defaultvalue" [ entity.amounttype != null  ] {
    enabled:true;
}

"entity.defaultvalue" [ entity.amounttype == null ] {
    enabled:false;
}

"entity.accttitle" [entity.docstate == 'APPROVED'] {
    readonly:true;
}

"entity.ngassubaccttitle" [entity.ngastitle == null] {
    enabled:false;
}

"entity.ngassubaccttitle" [entity.ngastitle != null] {
    enabled:true;
}

"entity.sresubaccttitle" [entity.sretitle == null] {
    enabled:false;
}

"entity.sresubaccttitle" [entity.sretitle != null] {
    enabled:true;
}
