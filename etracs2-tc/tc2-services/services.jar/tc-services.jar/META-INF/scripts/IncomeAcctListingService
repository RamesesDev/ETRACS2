import com.rameses.annotations.*;
import com.rameses.common.*;

class ConsolidatedReportService {
	@PersistenceContext('main')	
	def em
	
	@Service('ETRACSUtilService')
	def util
	
	@Service('LogService')
	def logService

	
	@ProxyMethod
	public def buildReport( entity ){
		def data = [];
		
		if( entity.charttype == 'NGAS' )
			data = em.sqlContext.createNamedQuery( 'incomeacct:getNGASData' ).resultList;
		else
			data = em.sqlContext.createNamedQuery( 'incomeacct:getSREData' ).resultList;
		
		return data;
	}
}
