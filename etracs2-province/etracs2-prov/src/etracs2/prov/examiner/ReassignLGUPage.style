"selectedItem|searchText" [mode == 'read'] {
    enabled:true;
}

"selectedItem|searchText" [mode != 'read'] {
    enabled:false;
}

"entity.*|searchMunicipality|selectedMunicipality" [mode == 'read'] {
    readonly:true;
}

"entity.*|searchMunicipality|selectedMunicipality" [mode != 'read'] {
    readonly:false;
}



