import com.rameses.annotations.*;

public class TDInterceptor
{
	@Service("TDService")
	def svc;
	
	@Service("LogService")
	def logService;
	
	@After(pattern="FAASService.approve")
	public void interceptFaasApprove( evt ) {
		def faas = evt.result;
		svc.post( faas );
		logService.log("GENERATE", "TD", faas.objid);		
	}
		
	
}