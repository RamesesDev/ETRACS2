		<field name="classTitle" required="true" />
		<field name="classCode" required="true" />
		<field name="actualUseTitle" required="true" />
		<field name="actualUseCode" required="true" />
		<field name="areaSqm" type="decimal" required="true" />
		<field name="areaHec" type="decimal" required="true" />
		<field name="marketValue" type="decimal" required="true" />
		<field name="adjMarketValue" type="decimal" required="true" />
		<field name="assessLevel" type="decimal" required="true" />
		<field name="assessedValue" type="decimal" required="true" />
		<field name="specificClass" required="true" />
		<field name="specificClassCode" required="true" />
		<field name="subclassClass" required="true" />
		<field name="subclassCode" required="true" />
		<field name="stripLevel" />
		<field name="baseValue" type="decimal" required="true" />
		<field name="unitValue" type="decimal" required="true" />
		<field name="taxable" type="int" required="true" />
	</element>
		
</schema>