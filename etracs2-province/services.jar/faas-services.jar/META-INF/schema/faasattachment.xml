<schema>

	<element name="faasAttachment" tablename="faasattachment">
		<link ref="attachmentInfo" />
	</element>
	
	<element name="attachmentInfo">
		<field name="objid" primary="true"/>
		<field name="schemaName" required="true"/>
		<field name="schemaVersion" required="true"/>
		<field name="refId" required="true" />
		<field name="documentType" required="true" />
		<field name="remarks" />
		<field name="fileName" required="true"/>
		<field name="fileType" required="true"/>
		<field name="fileSize" required="true" type="long"/>
		<field name="tmpFileName" required="true" />
	</element>

</schema>
