<schema>
	<element name="tree">
		<link ref="rpu:rpuinfo"/>
		<field name="totalBearing" required="true" type="integer" />
		<field name="totalNonBearing" type="integer" />
		<complex name="landPlantDetails" ref="landPlantDetail" type="list" min="1" serializer="default" />
	</element>
	
	<element name="landPlantDetail">
		<field name="objid" required="true"  />
		<field name="bearing" required="true" type="integer" />
		<field name="nonBearing" required="true" type="integer" />
		<field name="nonBearingAge"  />
		<field name="baseValue" required="true" type="decimal" />
		<field name="unitValue" required="true" type="decimal" />
		<field name="marketValue" required="true" type="decimal" />
		<field name="adjMarketValue" required="true" type="decimal" />
		<field name="assessLevel" required="true" type="decimal" />
		<field name="assessedValue" required="true" type="decimal" />
		<field name="treeUnitValueId" required="true"  />
		<field name="treeUnitValueCode" required="true"  />
		<field name="treeUnitValueTitle" required="true"  />
		<field name="treeUnitId" required="true"  />
		<field name="treeId" required="true"  />
		<field name="treeCode" required="true"  />
		<field name="treeTitle" required="true"  />
		<field name="treeAssessLevelId" required="true"  />
		<field name="treeAssessLevelCode" required="true"  />
		<field name="treeAssessLevelTitle" required="true"  />
	</element>
</schema>