<schema>
	<!-- Link to faasid = faas_list.objid for more faas information -->
	<element name="rpu" tablename="rpu">
		<link ref="rpu-info"/>
	</element>
	
	<element name="history" tablename="rpu_history">
		<link ref="rpu-info"/>
	</element>	
	
	<element name="rpu-info">
		<field name="objid" primary="true"/>
		<field name="schemaname" required="true"/>
		<field name="schemaversion" required="true"/>
		<field name="faasid" required="true"/>
		<field name="prevfaasid" />
		<field name="pin" required="true"/>
		<field name="tdno" required="true"/>
	</element>
</schema>