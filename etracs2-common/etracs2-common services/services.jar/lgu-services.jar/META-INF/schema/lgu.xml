<schema>
	<element name="lgu" tablename="lgu">
		<field name="objid" primary="true"/>
		<link ref="info"/>
		<field name="schemaname" required="true"/>
		<field name="schemaversion" required="true"/>
		<field name="docstate" required="true"/>
		<field name="parentid" />
		<field name="selectedlgutype" />
		<complex name="barangays" ref="barangay" required="false" serializer="default" />
		<complex name="extended" merge="true" required="false" serializer="default" />
	</element>
	
	<element name="info" >
		<field name="lgutype" />
		<field name="indexno"/>
		<field name="pin" />
		<field name="lguname"/>
		<field name="formalname"/>
	</element>
	
	<element name="barangay">
		<field name="objid" />
		<link ref="info"/>
	</element>
</schema>
