<schema>
    <element name="task" tablename="wf_task">
		<field name="objid" primary="true"/>
		<field name="dtposted" />
		<field name="requesterid" />
		<field name="msgtype" />
		<field name="handler" />
		<complex name="info" serializer="default" />
    </element>
	
	<element name="actor" tablename="wf_task_actor">
		<field name="objid" primary="true"/>
		<field name="taskid" />
		<field name="actorid" />
	</element>
	
</schema>
