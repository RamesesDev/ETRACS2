<schema>

	<element name="jobposition" tablename="jobposition" >
		<field name="objid" primary="true"/>
		<field name="code" required="true"/>
		<field name="title" required="true"/>
		<field name="jobgroup"/>
		<field name="roleclass" required="true"/>
		<field name="role" required="true"/>
		<field name="orgunitid" required="true"/>
		<complex name="excluded" serializer="default" required="false"/>
		<field name="assigneeid"/>
		<field name="jobstatus"/>
    </element>
	
	<element name="tag" tablename="jobposition_tag">
		<field name="jobid" primary="true"/>
		<field name="tagid" primary="true"/>
	</element>

</schema>