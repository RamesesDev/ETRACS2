<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://java.sun.com/xml/ns/javaee"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_5.xsd"
             version="5">
    <display-name>lguname</display-name>
	<module>
		<java>app-libs.jar</java>
	</module>
	<module>
		<java>services.jar</java>
	</module>
	<module>
		<ejb>rameses-server.jar</ejb>
	</module>
	<module>
		<ejb>ruleserver.jar</ejb>
	</module>
	<module>
		<web>
			<web-uri>http-invoker-server.war</web-uri>
			<context-root>lguname</context-root>
		</web>
	</module>
	<module>
		<web>
			<web-uri>help.war</web-uri>
			<context-root>help</context-root>
		</web>
	</module>	
	<module>
		<web>
			<web-uri>downloads.war</web-uri>
			<context-root>downloads</context-root>
		</web>
	</module>
    <module>
		<web>
			<web-uri>barbecue.war</web-uri>
			<context-root>barcode</context-root>
		</web>
	</module>	
